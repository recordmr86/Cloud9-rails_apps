<p id="notice"><%= notice %></p>

<h1>Listing Articles3s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles3s.each do |articles3| %>
      <tr>
        <td><%= articles3.title %></td>
        <td><%= articles3.description %></td>
        <td><%= link_to 'Show', articles3 %></td>
        <td><%= link_to 'Edit', edit_articles3_path(articles3) %></td>
        <td><%= link_to 'Destroy', articles3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Articles3', new_articles3_path %>
